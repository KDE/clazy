#include <QtCore/QObject>
#include <QtCore/QScopedPointer>
#include <QtCore/QPointer>
#include <QtCore/QSharedPointer>
class MyObj : public QObject {public: void doStuff();};
void test()
{
    QScopedPointer<QObject> ptr(new QObject);
    ptr->deleteLater(); // Warn

    QPointer<QObject> qptr(new QObject);
    qptr->deleteLater();   // Warn

    QSharedPointer<QObject> qsptr(new QObject);
    qsptr->deleteLater(); // Warn

    QScopedPointer<QObject> okPtr(new QObject);
    okPtr->deleteLater();

    static_cast<MyObj*>(qsptr.data())->doStuff();

    QPointer<MyObj> obj(new MyObj());
    QObject::connect(obj, &QObject::objectNameChanged, obj, &MyObj::doStuff);
    QObject::connect(obj, &QObject::objectNameChanged, obj, &MyObj::doStuff); // WARN
    QObject::connect(obj, &QObject::objectNameChanged, obj, &MyObj::doStuff); // WARN
    QObject::connect(obj, &QObject::objectNameChanged, obj, &MyObj::doStuff); // WARN, check fixit works for two warnings
    QObject::connect(ptr.data(), &QObject::objectNameChanged, ptr.data(), &MyObj::deleteLater); // OK, no overload for QScopedPointer
}

